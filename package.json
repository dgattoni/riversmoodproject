{
  "name": "river-mood-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "gulp lint",
    "test": "gulp lint",
    "clean": "gulp clean",
    "build": "gulp build",
    "serve": "gulp serve",
    "start": "npm run build -- --production; node dist/server/index.js"
  },
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=3.10.9"
  },
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "browserify": "^14.3.0",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cssnano": "^3.8.0",
    "dotenv": "^4.0.0",
    "event-stream": "^3.3.4",
    "express": "^4.14.0",
    "express-http-proxy": "^1.0.1",
    "fs-extra": "^2.1.2",
    "gaze": "^1.1.2",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-data": "^1.2.1",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-inject": "^4.1.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-postcss": "^6.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "helmet": "^3.5.0",
    "method-override": "^2.3.6",
    "moment": "^2.18.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "newrelic": "^1.32.0",
    "node-emoji": "^1.5.1",
    "pug": "^2.0.0-beta.12",
    "request": "^2.81.0",
    "run-sequence": "^1.2.2",
    "serve-favicon": "^2.3.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "computed-property-spacing": [
        2,
        "never"
      ],
      "comma-style": 2,
      "eol-last": 1,
      "key-spacing": [
        2,
        {
          "beforeColon": false,
          "afterColon": true,
          "mode": "strict"
        }
      ],
      "no-console": 1,
      "no-extra-parens": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "no-unneeded-ternary": 2,
      "object-curly-spacing": [
        2,
        "never"
      ],
      "padded-blocks": [
        2,
        "never"
      ],
      "quotes": [
        2,
        "single"
      ],
      "quote-props": [
        2,
        "as-needed"
      ],
      "semi": [
        2,
        "always"
      ],
      "space-in-parens": [
        2,
        "never"
      ]
    }
  }
}
